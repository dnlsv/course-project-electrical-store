<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.ViewProductListController">
    <children>
        <Label layoutX="169.0" layoutY="14.0" text="Список товаров">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <TableView fx:id="productListTable" layoutY="182.0" onMouseClicked="#productListTableAction" prefHeight="218.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0">
            <columns>
                <TableColumn fx:id="productListOrderIDColumn" prefWidth="166.0" text="Номер заказа" />
                <TableColumn fx:id="productListProductIDColumn" prefWidth="175.0" text="Код товара" />
                <TableColumn fx:id="productListQuantity" prefWidth="158.0" text="Количество" />
            </columns>
        </TableView>
        <Button fx:id="returnButton" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#returnButtonAction" text="Назад" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0" />
    </children>
</AnchorPane>
